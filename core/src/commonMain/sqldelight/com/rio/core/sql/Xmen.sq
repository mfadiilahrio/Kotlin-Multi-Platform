CREATE TABLE xmenRecord (
    _id INTEGER PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    imdbID TEXT NOT NULL,
    year TEXT NOT NULL,
    type TEXT NOT NULL,
    poster TEXT NOT NULL
);

insert:
INSERT INTO xmenRecord(title, imdbID, year, type, poster)
VALUES (?, ?, ?, ?, ?);

selectAll:
SELECT * FROM xmenRecord;

deleteAll:
DELETE FROM xmenRecord;